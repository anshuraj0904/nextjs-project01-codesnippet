generator client {
  provider = "prisma-client-js" // âœ… Default output to node_modules/@prisma/client
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Snippet {
  id    Int    @id @default(autoincrement())
  title String
  code  String
}
